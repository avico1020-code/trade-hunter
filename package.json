{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:log": "next dev --turbopack > logs/server.log 2>&1",
    "dev:full": "concurrently \"bunx convex dev\" \"next dev --turbopack\"",
    "dev:npx": "npx next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "convex": "bunx convex dev",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint:full": "biome check . && bun run type-check",
    "check:full": "biome check --write . && bun run type-check",
    "ultracite": "ultracite",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
