    "lucide-react": "^0.546.0",
    "nanoid": "^5.1.6",
    "next": "^16.1.0",
    "openai": "^6.9.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "styled-jsx": "^5.1.7",
